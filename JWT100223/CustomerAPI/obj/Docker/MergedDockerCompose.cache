services:
  customerapi:
    build:
      context: G:\Local disk\AccentureTrainingFeb2023\JWT100223\CustomerAPI
      dockerfile: CustomerAPI/Dockerfile
    depends_on:
      sqlservercustomerservice:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: customerapi
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Balasubramaniam\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Balasubramaniam\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  sqlservercustomerservice:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Vignesh@95
    expose:
    - '1433'
    hostname: sqlservercustomerservice
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 1406
      target: 1433
    volumes:
    - G:\Local disk\AccentureTrainingFeb2023\JWT100223\CustomerAPI\customerapi:/var/opt/mssql/data:rw
version: '3.4'